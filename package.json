{
  "name": "ranjs",
  "version": "1.24.5",
  "description": "Library for generating various random variables.",
  "keywords": [
    "random",
    "generator",
    "distributions",
    "mcmc",
    "statistics",
    "test"
  ],
  "homepage": "https://synesenom.github.io/ran/",
  "author": {
    "name": "Enys Mones",
    "url": "enysmones.com",
    "email": "enys.mones@gmail.com"
  },
  "license": "MIT",
  "main": "./dist/ranjs.min.js",
  "module": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/synesenom/ran"
  },
  "scripts": {
    "standard": "./node_modules/.bin/standard --fix src/**/*.js",
    "test": "./node_modules/.bin/_mocha -r esm",
    "docs": "node ./docs/index.js",
    "build": "./node_modules/.bin/rollup -c",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=html _mocha --require @babel/register",
    "coveralls": "cross-env NODE_ENV=test nyc --reporter=text-lcov _mocha --recursive --require @babel/register | coveralls"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.15.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "documentation": "^14.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "esm": "^3.2.25",
    "highlight.js": "^10.2.0",
    "lodash.flattendeep": "^4.4.0",
    "istanbul": "^0.4.5",
    "mathjax-node-page": "^3.2.0",
    "mocha": "^8.4.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.15",
    "node-sass": "^7.0.0",
    "npm": "^8.11.0",
    "nyc": "^15.1.0",
    "pug": "^3.0.2",
    "rollup": "^2.64.0",
    "rollup-plugin-terser": "^7.0.2",
    "seedrandom": "^3.0.5",
    "standard": "^16.0.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "include": [
      "src/**/*.js"
    ],
    "sourceMap": false,
    "instrument": false,
    "require": [
      "@babel/register"
    ]
  }
}
